public class NomeCompleto{
	private String nome;
	
	public NomeCompleto(String nome){
		this.nome = nome;
	}

	public String getNome() {
		return nome;
	}

	public void setNome(String nome) {
		this.nome = nome;
	}

	@Override
	public String toString() {
		return "NomeCompleto [nome=" + nome + "]";
	}
	
}
